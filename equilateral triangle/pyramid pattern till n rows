#include <stdio.h>
int print_tringle(num)
{
    int i, j;
    for(i=1;i<=num;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("* ");
        }
        printf("\n");
    }
}
int main()
{
    int n;
    printf("Enter no. of rows: ");
    scanf("%d", &n);
    print_tringle(n);
    return 0;
}
